tinc:
  network:
    mynet:
      type: mesh # "mesh" or "central"
      conf:
        local:
          # See options from https://www.tinc-vpn.org/documentation/tinc.conf.5
          # Name and ConnectTo values are fixed and predefined
          AddressFamily: "any"
          Mode: "switch"
        host:
          # See options from https://www.tinc-vpn.org/documentation/Host-configuration-variables.html
          # Address is predefined
          # Port and Subnet are required
          Cipher: aes-128-cbc
          Compression: 0
          Port: 1160
          Subnet: 10.5.0.0/16
      node:
        Vivian.remote.alinuxninjasrv:
          ip:
            public: 192.168.2.1
            local: 10.5.1.10/255.255.255.0
        cr02.startledphoenixsrv:
          ip:
            public: 192.168.2.1
            local: 10.5.1.10/255.255.255.0
          conf:
            host:
              Port: 5200
          master: True
