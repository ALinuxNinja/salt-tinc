tinc:
  service:
    dns:
      common:
        enabled: True
        external-servers:
          - 8.8.8.8
          - 8.8.4.4
        internal-domains:
          - startledphoenixsrv
          - alinuxninjasrv
    ospf:
      enabled: True
      common:
        listen-interfaces:
          - core
        networks: []
        passive-interfaces: []
      customhost:
        networks:
          - 172.16.11.0/24
        passive-interfaces:
          - eth1
  network:
    core:
      subnet: 10.5.0.0/16
      port: 1160
      cipher: aes-128-cbc
      compression: 10
      digest: sha1
      dns-servers:
        - 8.8.8.8
        - 8.8.4.4
      master:
        cr02.startledphoenixsrv:
          public-ip: 192.168.2.1
          local-ip: 10.5.1.10
      node:
        Vivian.remote.alinuxninjasrv:
          public-ip: 192.168.2.2
          local-ip: 10.5.1.11
